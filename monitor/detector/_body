<script>
$(document).ready(
  function(){
    var timer5 = 0;
    $(".allschool p").mouseenter(
        function(){
            clearTimeout(timer5);
            $(".school").show();
            $(".allschool p").addClass('cur');
        }
    );
    $(".allschool p").mouseleave(function(){
        kHiden();
        $(".allschool p").removeClass('cur');
    });
    $(".school").mouseenter(function(){
        clearTimeout(timer5);
        $(".allschool p").addClass('cur');
    });
    $(".school").mouseleave(function(){
        $(".allschool p").removeClass('cur');
        kHiden();
    });
    function kHiden(){
        timer5 = setTimeout(function(){$(".school").hide()},100)
    }
  }
);
</script>
